<div id="scormreport_heatmap_sectionwrapper_{{id}}">
    <div class="scormreport_heatmap_title">
        <h2 title="{{id}}">{{title}}</h2>
        <span class="scormreport_heatmap_rightside">{{#str}}answers, scormreport_heatmap{{/str}}: {{answers}}</span>
    </div>
    <table class="table table-striped table-hover scormreport_heatmap_table_fixed">
        {{#lines}}
            <tr>
                <td class="cell c{{@index}} scormreport_heatmap_td_breaktext">
                    {{.}}
                </td>
            </tr>
        {{/lines}}
    </table>
</div>